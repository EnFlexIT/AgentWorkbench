<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>org.agentgui</groupId>
	<artifactId>org.agentgui.lib.jung</artifactId>
	<version>2.2.0-SNAPSHOT</version>
	<packaging>pom</packaging>

	<properties>
		<jung-version>2.2.0</jung-version>
	</properties>

	<dependencies>
		<dependency>
			<groupId>com.northdata.jung</groupId>
			<artifactId>jung-algorithms</artifactId>
			<version>${jung-version}</version>
			<type>pom</type>
		</dependency>
		<dependency>
			<groupId>com.northdata.jung</groupId>
			<artifactId>jung-api</artifactId>
			<version>${jung-version}</version>
			<type>pom</type>
		</dependency>
		<dependency>
			<groupId>com.northdata.jung</groupId>
			<artifactId>jung-graph-impl</artifactId>
			<version>${jung-version}</version>
			<type>pom</type>
		</dependency>
		<dependency>
			<groupId>com.northdata.jung</groupId>
			<artifactId>jung-io</artifactId>
			<version>${jung-version}</version>
			<type>pom</type>
		</dependency>
		<dependency>
			<groupId>com.northdata.jung</groupId>
			<artifactId>jung-samples</artifactId>
			<version>${jung-version}</version>
			<type>pom</type>
		</dependency>
		<dependency>
			<groupId>com.northdata.jung</groupId>
			<artifactId>jung-visualization</artifactId>
			<version>${jung-version}</version>
			<type>pom</type>
		</dependency>
	</dependencies>


	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-dependency-plugin</artifactId>
				<version>3.2.0</version>
			</plugin>
		</plugins>
	</build>
	
	
	
	<description>Execute this Maven POM to downlod all Jung libs and its dependencies to the directory '/target/dependency' by calling 

a) mvn dependency:copy-dependencies
(for the jar-files) or 
b) mvn dependency:copy-dependencies -Dclassifier=sources
(for the jar-sources)</description>
</project>