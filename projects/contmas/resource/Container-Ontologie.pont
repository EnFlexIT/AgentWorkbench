(defclass LoadList "collection of transport orders for one long-time loading or unloading process (layer-kind of cfp)"
	(is-a Concept)
	(role concrete)
	(multislot consists_of
		(type INSTANCE)
;+		(allowed-classes TransportOrderChain)
		(cardinality 1 ?VARIABLE)
		(create-accessor read-write)))

(defclass TransportOrderChain "high-level transport order with destination"
	(is-a Concept)
	(role concrete)
	(single-slot transports
;+		(comment "container to be transported")
		(type INSTANCE)
;+		(allowed-classes Container)
;+		(cardinality 1 1)
		(create-accessor read-write))
	(single-slot terminates_at
;+		(comment "final destination e.g. target harbour")
		(type STRING)
;+		(cardinality 0 1)
		(create-accessor read-write))
	(multislot is_linked_by
		(type INSTANCE)
;+		(allowed-classes TransportOrder)
		(cardinality 1 ?VARIABLE)
		(create-accessor read-write)))

(defclass TransportOrder "step-like transport orders"
	(is-a Concept)
	(role concrete)
	(single-slot ends_at
		(type INSTANCE)
;+		(allowed-classes Designator)
;+		(cardinality 0 1)
		(create-accessor read-write))
	(single-slot takes
;+		(comment "required time to perform")
		(type FLOAT)
;+		(cardinality 0 1)
		(create-accessor read-write))
	(single-slot starts_at
		(type INSTANCE)
;+		(allowed-classes Designator)
;+		(cardinality 1 1)
		(create-accessor read-write)))

(defclass Designator "start or end of a transport order"
	(is-a Concept)
	(role concrete)
	(single-slot abstract_designation
		(type INSTANCE)
;+		(allowed-classes Domain)
;+		(cardinality 0 1)
		(create-accessor read-write))
	(single-slot type
		(type SYMBOL)
		(allowed-values abstract concrete)
;+		(cardinality 0 1)
		(create-accessor read-write))
	(single-slot concrete_designation
		(type INSTANCE)
;+		(allowed-classes AID)
;+		(cardinality 0 1)
		(create-accessor read-write)))