<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<project>
    <projectName>Monitormas</projectName>
    <subOntologies>
        <subOntology>monitormas.ontology.MonitorMASOntology</subOntology>
        <subOntology>distribution.ontology.AgentGUI_DistributionOntology</subOntology>
    </subOntologies>
    <agentConfiguration>
        <entry>
            <key>monitormas.agents.ControlAgent</key>
            <value>monitormas.ontology.TimeDetector|distribution.ontology.PlatformAddress|distribution.ontology.PlatformTime</value>
        </entry>
    </agentConfiguration>
    <jadeConfiguration>
        <start4Sim>true</start4Sim>
        <useAppDefaults>true</useAppDefaults>
        <useLocalPort>1099</useLocalPort>
        <serviceList>
            <service>jade.core.messaging.TopicManagementService;</service>
            <service>jade.core.event.NotificationService;</service>
            <service>jade.core.mobility.AgentMobilityService;</service>
        </serviceList>
    </jadeConfiguration>
    <mainPackage>monitormas</mainPackage>
    <subFolderAgents>agents</subFolderAgents>
    <subFolderEnvSetups>envSetups</subFolderEnvSetups>
    <subFolderOntology>ontology</subFolderOntology>
    <subFolderResources>resources</subFolderResources>
</project>
