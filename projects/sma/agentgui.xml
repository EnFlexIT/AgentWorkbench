<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<project>
    <projectName>SMA</projectName>
    <projectDescription>Simple Moving Agent

Einfaches Demo-Projekt fr die Visualisierung</projectDescription>
    <subOntologies/>
    <agentConfiguration/>
    <jadeConfiguration>
        <start4Sim>true</start4Sim>
        <useAppDefaults>true</useAppDefaults>
        <useLocalPort>1099</useLocalPort>
        <serviceList>
            <service>jade.core.messaging.TopicManagementService;</service>
            <service>jade.core.event.NotificationService;</service>
            <service>jade.core.migration.InterPlatformMobilityService;</service>
            <service>jade.core.mobility.AgentMobilityService;</service>
        </serviceList>
    </jadeConfiguration>
    <envFile>CollectBoxExample.xml</envFile>
    <mainPackage>sma</mainPackage>
    <subFolderAgents>agents</subFolderAgents>
    <subFolderEnvSetups>envSetups</subFolderEnvSetups>
    <subFolderOntology>ontology</subFolderOntology>
    <subFolderResources>resources</subFolderResources>
    <svgFile>CollectBoxExample.svg</svgFile>
</project>
