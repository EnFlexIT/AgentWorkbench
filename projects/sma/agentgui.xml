<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<project>
    <projectName>SMA</projectName>
    <projectDescription>Simple Moving Agent

Einfaches Demo-Projekt fr die Visualisierung</projectDescription>
    <projectResources/>
    <subOntologies/>
    <agentConfiguration/>
    <simulationSetupCurrent>BoxExample</simulationSetupCurrent>
    <simulationSetups>
        <entry>
            <key>HarborExample</key>
            <value>harborexample.xml</value>
        </entry>
        <entry>
            <key>ManyExample</key>
            <value>manyexample.xml</value>
        </entry>
        <entry>
            <key>BoxExample</key>
            <value>boxexample.xml</value>
        </entry>
    </simulationSetups>
    <jadeConfiguration>
        <start4Sim>true</start4Sim>
        <useAppDefaults>false</useAppDefaults>
        <useLocalPort>1104</useLocalPort>
        <serviceList>
            <service>jade.core.event.NotificationService;</service>
            <service>mas.environment.provider.EnvironmentProviderService;</service>
            <service>jade.core.mobility.AgentMobilityService;</service>
            <service>mas.service.SimulationService;</service>
            <service>agentgui.physical2Denvironment.provider.EnvironmentProviderService;</service>
        </serviceList>
    </jadeConfiguration>
    <mainPackage>sma</mainPackage>
    <subFolderEnvSetups>svgEnvSetups</subFolderEnvSetups>
</project>
