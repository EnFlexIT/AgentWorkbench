    <subOntologies>
        <subOntology>agentgui.ontology.AgentGUI_BaseOntology</subOntology>
    </subOntologies>
    <agentStartConfiguration>
        <agentVector>
            <agent agentReference="gol.acl.agents.GameOfLifeAgent">
                <startArgumentVector>
                    <startArgument position="1" ontologyReference="agentgui.ontology.TimeSeriesChart"/>
                    <startArgument position="2" ontologyReference="agentgui.ontology.Simple_Boolean"/>
                    <startArgument position="3" ontologyReference="agentgui.ontology.Simple_String"/>
                </startArgumentVector>
            </agent>
        </agentVector>
    </agentStartConfiguration>
    <jadeConfiguration>
        <useLocalPort>1099</useLocalPort>
