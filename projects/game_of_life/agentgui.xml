<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<project>
    <projectName>Game of Life</projectName>
    <projectView>Developer</projectView>
    <environmentModel>Squared</environmentModel>
    <projectResources>
        <projectResource>D:\10 Java Projects\Game of Life\bin</projectResource>
    </projectResources>
    <plugins>
        <className>gol.plugin.GameOfLifePlugIn</className>
    </plugins>
    <subOntologies>
        <subOntology>agentgui.ontology.AgentGUI_BaseOntology</subOntology>
    </subOntologies>
    <agentConfiguration>
        <entry>
            <key>game_of_life.SimService.agents.GameOfLifeAgent</key>
            <value>[Toast] agentgui.ontology.Simple_Boolean|[Vollkorn] agentgui.ontology.Simple_Integer|agentgui.ontology.Simple_String</value>
        </entry>
    </agentConfiguration>
    <jadeConfiguration>
        <useLocalPort>1117</useLocalPort>
        <serviceList>
            <service>agentgui.simulationService.LoadService</service>
            <service>jade.core.event.NotificationService</service>
            <service>jade.debugging.DebugService</service>
            <service>agentgui.simulationService.SimulationService</service>
            <service>jade.core.mobility.AgentMobilityService</service>
        </serviceList>
    </jadeConfiguration>
    <distributionSetup>
        <doDynamicLoadBalancing>false</doDynamicLoadBalancing>
        <doStaticLoadBalancing>true</doStaticLoadBalancing>
        <dynamicLoadBalancingClass>agentgui.simulationService.balancing.DynamicLoadBalancing</dynamicLoadBalancingClass>
        <numberOfAgents>800</numberOfAgents>
        <numberOfContainer>1</numberOfContainer>
        <staticLoadBalancingClass>gol.SimService.balancing.StartAndBalancing</staticLoadBalancingClass>
        <useUserThresholds>false</useUserThresholds>
        <userThresholds>
            <thCpuH>80</thCpuH>
            <thCpuL>5</thCpuL>
            <thMemoH>80</thMemoH>
            <thMemoL>5</thMemoL>
            <thNoThreadsH>1500</thNoThreadsH>
            <thNoThreadsL>50</thNoThreadsL>
        </userThresholds>
    </distributionSetup>
    <remoteContainerConfiguration>
        <jvmMemAllocInitial>32m</jvmMemAllocInitial>
        <jvmMemAllocMaximum>128m</jvmMemAllocMaximum>
        <preventUsageOfAlreadyUsedComputers>false</preventUsageOfAlreadyUsedComputers>
        <showJADErmaGUI>false</showJADErmaGUI>
    </remoteContainerConfiguration>
    <simulationSetupCurrent>Game of Life 360</simulationSetupCurrent>
    <simulationSetups>
        <entry>
            <key>Game of Life 360</key>
            <value>game_of_life_360.xml</value>
        </entry>
    </simulationSetups>
    <subFolderEnvSetups>setupsEnv</subFolderEnvSetups>
</project>
