<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<project>
    <projectName>Game of Life</projectName>
    <projectView>Developer</projectView>
    <projectResources/>
    <plugins/>
    <subOntologies>
        <subOntology>agentgui.ontology.AgentGUI_BaseOntology</subOntology>
    </subOntologies>
    <agentConfiguration>
        <entry>
            <key>game_of_life.SimService.agents.GameOfLifeAgent</key>
            <value>[Toast] agentgui.ontology.Simple_Boolean|[Vollkorn] agentgui.ontology.Simple_Integer|agentgui.ontology.Simple_String</value>
        </entry>
    </agentConfiguration>
    <jadeConfiguration>
        <useLocalPort>1108</useLocalPort>
        <serviceList>
            <service>agentgui.simulationService.LoadService</service>
            <service>jade.core.event.NotificationService</service>
            <service>jade.debugging.DebugService</service>
            <service>agentgui.simulationService.SimulationService</service>
            <service>jade.core.mobility.AgentMobilityService</service>
        </serviceList>
    </jadeConfiguration>
    <distributionSetup>
        <doDynamicLoadBalancing>false</doDynamicLoadBalancing>
        <doStaticLoadBalancing>true</doStaticLoadBalancing>
        <dynamicLoadBalancingClass>agentgui.simulationService.balancing.DynamicLoadBalancing</dynamicLoadBalancingClass>
        <numberOfAgents>200</numberOfAgents>
        <numberOfContainer>2</numberOfContainer>
        <staticLoadBalancingClass>game_of_life.SimService.balancing.StartAndBalancing</staticLoadBalancingClass>
        <useUserThresholds>false</useUserThresholds>
        <userThresholds>
            <thCpuH>80</thCpuH>
            <thCpuL>5</thCpuL>
            <thMemoH>80</thMemoH>
            <thMemoL>5</thMemoL>
            <thNoThreadsH>1500</thNoThreadsH>
            <thNoThreadsL>50</thNoThreadsL>
        </userThresholds>
    </distributionSetup>
    <remoteContainerConfiguration/>
    <simulationSetupCurrent>GoL ACL-Messages</simulationSetupCurrent>
    <simulationSetups>
        <entry>
            <key>GoL ACL-Messages</key>
            <value>gol_acl_messages.xml</value>
        </entry>
        <entry>
            <key>GoL SimService</key>
            <value>gol_simservice.xml</value>
        </entry>
    </simulationSetups>
    <subFolderEnvSetups>setupsEnv</subFolderEnvSetups>
</project>
