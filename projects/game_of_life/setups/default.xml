<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<simulationSetup>
    <agentSetup>
        <agent>
            <agentClassReference>game_of_life.agents.StartAgent</agentClassReference>
            <mobileAgent>true</mobileAgent>
            <postionNo>1</postionNo>
            <startAsName>StAg</startAsName>
        </agent>
    </agentSetup>
    <distribution>
        <doDynamicLoadBalalncing>false</doDynamicLoadBalalncing>
        <doStaticLoadBalalncing>true</doStaticLoadBalalncing>
        <dynamicLoadBalancingClass>agentgui.simulationService.balancing.DynamicLoadBalancing</dynamicLoadBalancingClass>
        <numberOfAgents>1300</numberOfAgents>
        <numberOfContainer>2</numberOfContainer>
        <staticLoadBalancingClass>agentgui.simulationService.balancing.StaticLoadBalancing</staticLoadBalancingClass>
        <useUserThresholds>false</useUserThresholds>
        <userThresholds>
            <thCpuH>80</thCpuH>
            <thCpuL>5</thCpuL>
            <thMemoH>80</thMemoH>
            <thMemoL>5</thMemoL>
            <thNoThreadsH>1500</thNoThreadsH>
            <thNoThreadsL>50</thNoThreadsL>
        </userThresholds>
    </distribution>
    <agentListModel>
        <size>1</size>
    </agentListModel>
    <environmentFileName>Game of Life_default.xml</environmentFileName>
    <svgFileName>Game of Life_default.svg</svgFileName>
</simulationSetup>
