<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<project>
    <projectName>GasMAS</projectName>
    <projectDescription>MAS zur Simulation eines Gasnetzes (nur technische Ebene).</projectDescription>
    <environmentModel>gridEnvironment</environmentModel>
    <projectResources/>
    <subOntologies>
        <subOntology>gasmas.ontology.GasgridOntology</subOntology>
        <subOntology>agentgui.simulationService.ontology.AgentGUI_DistributionOntology</subOntology>
    </subOntologies>
    <agentConfiguration>
        <entry>
            <key>gasmas.agents.EntryAgent</key>
            <value>gasmas.ontology.Entry</value>
        </entry>
        <entry>
            <key>gasmas.agents.BranchAgent</key>
            <value>gasmas.ontology.Branch</value>
        </entry>
        <entry>
            <key>gasmas.agents.ExitAgent</key>
            <value>gasmas.ontology.Exit</value>
        </entry>
        <entry>
            <key>gasmas.agents.PipeAgent</key>
            <value>gasmas.ontology.Pipe</value>
        </entry>
        <entry>
            <key>gasmas.agents.CompressorAgent</key>
            <value>gasmas.ontology.Compressor</value>
        </entry>
    </agentConfiguration>
    <simulationSetupCurrent>default</simulationSetupCurrent>
    <simulationSetups>
        <entry>
            <key>default</key>
            <value>default.xml</value>
        </entry>
    </simulationSetups>
    <jadeConfiguration>
        <start4Sim>true</start4Sim>
        <useAppDefaults>true</useAppDefaults>
        <useLocalPort>1099</useLocalPort>
        <serviceList>
            <service>jade.core.event.NotificationService;</service>
            <service>jade.core.mobility.AgentMobilityService;</service>
            <service>agentgui.simulationService.SimulationService;</service>
        </serviceList>
    </jadeConfiguration>
    <agentClassHash>
        <entry>
            <key>valve</key>
            <value>gasmas.agents.ValveAgent</value>
        </entry>
        <entry>
            <key>entry</key>
            <value>gasmas.agents.EntryAgent</value>
        </entry>
        <entry>
            <key>branch</key>
            <value>gasmas.agents.BranchAgent</value>
        </entry>
        <entry>
            <key>compressor</key>
            <value>gasmas.agents.CompressorAgent</value>
        </entry>
        <entry>
            <key>exit</key>
            <value>gasmas.agents.ExitAgent</value>
        </entry>
        <entry>
            <key>pipe</key>
            <value>gasmas.agents.PipeAgent</value>
        </entry>
        <entry>
            <key>storage</key>
            <value>gasmas.agents.StorageAgent</value>
        </entry>
    </agentClassHash>
    <ontoClassHash>
        <entry>
            <key>valve</key>
            <value>gasmas.ontology.Valve</value>
        </entry>
        <entry>
            <key>entry</key>
            <value>gasmas.ontology.Entry</value>
        </entry>
        <entry>
            <key>branch</key>
            <value>gasmas.ontology.Branch</value>
        </entry>
        <entry>
            <key>compressor</key>
            <value>gasmas.ontology.Compressor</value>
        </entry>
        <entry>
            <key>exit</key>
            <value>gasmas.ontology.Exit</value>
        </entry>
        <entry>
            <key>pipe</key>
            <value>gasmas.ontology.Pipe</value>
        </entry>
        <entry>
            <key>storage</key>
            <value>gasmas.ontology.Storage</value>
        </entry>
    </ontoClassHash>
    <subFolderEnvSetups>svgEnvSetups</subFolderEnvSetups>
</project>
