<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<project>
    <projectName>GasMAS</projectName>
    <projectDescription>MAS zur Simulation eines Gasnetzes (nur technische Ebene).</projectDescription>
    <projectView>Developer</projectView>
    <environmentModel>gridEnvironment</environmentModel>
    <projectResources/>
    <plugins>
        <className>gasmas.plugin.GasMasPlugIn</className>
    </plugins>
    <subOntologies>
        <subOntology>gasmas.ontology.GasGridOntology</subOntology>
        <subOntology>agentgui.ontology.AgentGUI_BaseOntology</subOntology>
    </subOntologies>
    <agentStartConfiguration>
        <agentVector>
            <agent agentReference="gasmas.agents.components.ClusterNetworkComponentAgent">
                <startArgumentVector/>
            </agent>
            <agent agentReference="gasmas.agents.components.ClusterNetworkAgent">
                <startArgumentVector>
                    <startArgument ontologyReference="agentgui.ontology.TimeSeriesChart" position="1"/>
                    <startArgument ontologyReference="agentgui.ontology.XyChart" position="2"/>
                </startArgumentVector>
            </agent>
        </agentVector>
    </agentStartConfiguration>
    <jadeConfiguration>
        <useLocalPort>1468</useLocalPort>
        <serviceList>
            <service>agentgui.simulationService.LoadService</service>
            <service>jade.core.event.NotificationService</service>
            <service>agentgui.simulationService.SimulationService</service>
            <service>jade.debugging.DebugService</service>
            <service>jade.core.mobility.AgentMobilityService</service>
        </serviceList>
    </jadeConfiguration>
    <distributionSetup>
        <doDynamicLoadBalancing>false</doDynamicLoadBalancing>
        <doStaticLoadBalancing>false</doStaticLoadBalancing>
        <dynamicLoadBalancingClass>agentgui.simulationService.balancing.DynamicLoadBalancing</dynamicLoadBalancingClass>
        <numberOfAgents>1500</numberOfAgents>
        <numberOfContainer>1</numberOfContainer>
        <staticLoadBalancingClass>agentgui.simulationService.balancing.StaticLoadBalancing</staticLoadBalancingClass>
        <useUserThresholds>false</useUserThresholds>
        <userThresholds>
            <thCpuH>80</thCpuH>
            <thCpuL>5</thCpuL>
            <thMemoH>80</thMemoH>
            <thMemoL>5</thMemoL>
            <thNoThreadsH>1500</thNoThreadsH>
            <thNoThreadsL>50</thNoThreadsL>
        </userThresholds>
    </distributionSetup>
    <remoteContainerConfiguration>
        <jvmMemAllocInitial>32m</jvmMemAllocInitial>
        <jvmMemAllocMaximum>128m</jvmMemAllocMaximum>
        <preventUsageOfAlreadyUsedComputers>false</preventUsageOfAlreadyUsedComputers>
        <showJADErmaGUI>true</showJADErmaGUI>
    </remoteContainerConfiguration>
    <simulationSetupCurrent>MixedClusters (1) Small</simulationSetupCurrent>
    <timeModelClass>agentgui.simulationService.time.TimeModelContinuous</timeModelClass>
    <downloadResources>http://rudel1.cs.uni-due.de:8080/gasmas/gasmas_IDE.jar</downloadResources>
    <simulationSetups>
        <entry>
            <key>SmallClusters (2) Mid</key>
            <value>smallclusters_2_mid.xml</value>
        </entry>
        <entry>
            <key>MixedClusters (2) Medium</key>
            <value>mixedclusters_2_medium.xml</value>
        </entry>
        <entry>
            <key>HNord (2) Mid</key>
            <value>hnord_2_mid.xml</value>
        </entry>
        <entry>
            <key>SmartGrid</key>
            <value>smartgrid.xml</value>
        </entry>
        <entry>
            <key>Optionales Gas Netz</key>
            <value>optionales_gas_netz.xml</value>
        </entry>
        <entry>
            <key>SmallClusters (3) Large</key>
            <value>smallclusters_3_large.xml</value>
        </entry>
        <entry>
            <key>Testnetz 3</key>
            <value>testnetz_3.xml</value>
        </entry>
        <entry>
            <key>SmallClusters (1) Small</key>
            <value>smallclusters_1_small.xml</value>
        </entry>
        <entry>
            <key>HNord</key>
            <value>hnord.xml</value>
        </entry>
        <entry>
            <key>Simple Net</key>
            <value>simple_net.xml</value>
        </entry>
        <entry>
            <key>HNord (1) Small</key>
            <value>hnord_1_small.xml</value>
        </entry>
        <entry>
            <key>Testimport</key>
            <value>testimport.xml</value>
        </entry>
        <entry>
            <key>MixedClusters (3) Large</key>
            <value>mixedclusters_3_large.xml</value>
        </entry>
        <entry>
            <key>MixedClusters (1) Small</key>
            <value>mixedclusters_1_small.xml</value>
        </entry>
        <entry>
            <key>Simple Net Two Entries</key>
            <value>simple_net_two_entries.xml</value>
        </entry>
        <entry>
            <key>HNord Complete</key>
            <value>hnord_complete.xml</value>
        </entry>
        <entry>
            <key>HNord (3) Large</key>
            <value>hnord_3_large.xml</value>
        </entry>
    </simulationSetups>
    <subFolderEnvSetups>setupsEnv</subFolderEnvSetups>
    <unsaved>false</unsaved>
</project>
