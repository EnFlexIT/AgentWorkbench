<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<project>
    <projectName>GasMAS</projectName>
    <projectDescription>MAS zur Simulation eines Gasnetzes (nur technische Ebene).</projectDescription>
    <projectView>Developer</projectView>
    <environmentModel>gridEnvironment</environmentModel>
    <projectResources/>
    <plugins/>
    <subOntologies>
        <subOntology>gasmas.ontology.GasgridOntology</subOntology>
    </subOntologies>
    <agentConfiguration>
        <entry>
            <key>gasmas.agents.EntryAgent</key>
            <value>gasmas.ontology.Entry</value>
        </entry>
        <entry>
            <key>gasmas.agents.BranchAgent</key>
            <value>gasmas.ontology.Branch</value>
        </entry>
        <entry>
            <key>gasmas.agents.ValveAgent</key>
            <value>gasmas.ontology.Valve</value>
        </entry>
        <entry>
            <key>gasmas.agents.ExitAgent</key>
            <value>gasmas.ontology.Exit</value>
        </entry>
        <entry>
            <key>gasmas.agents.PipeAgent</key>
            <value>gasmas.ontology.Pipe</value>
        </entry>
        <entry>
            <key>gasmas.agents.CompressorAgent</key>
            <value>gasmas.ontology.Compressor</value>
        </entry>
        <entry>
            <key>gasmas.agents.StorageAgent</key>
            <value>gasmas.ontology.Storage</value>
        </entry>
    </agentConfiguration>
    <jadeConfiguration>
        <useLocalPort>1106</useLocalPort>
        <serviceList>
            <service>agentgui.simulationService.LoadService</service>
            <service>jade.core.event.NotificationService</service>
            <service>agentgui.simulationService.SimulationService</service>
            <service>jade.core.mobility.AgentMobilityService</service>
        </serviceList>
    </jadeConfiguration>
    <distributionSetup>
        <doDynamicLoadBalancing>false</doDynamicLoadBalancing>
        <doStaticLoadBalancing>false</doStaticLoadBalancing>
        <dynamicLoadBalancingClass>agentgui.simulationService.balancing.DynamicLoadBalancing</dynamicLoadBalancingClass>
        <numberOfAgents>0</numberOfAgents>
        <numberOfContainer>0</numberOfContainer>
        <staticLoadBalancingClass>agentgui.simulationService.balancing.StaticLoadBalancing</staticLoadBalancingClass>
        <useUserThresholds>false</useUserThresholds>
        <userThresholds>
            <thCpuH>80</thCpuH>
            <thCpuL>5</thCpuL>
            <thMemoH>80</thMemoH>
            <thMemoL>5</thMemoL>
            <thNoThreadsH>1500</thNoThreadsH>
            <thNoThreadsL>50</thNoThreadsL>
        </userThresholds>
    </distributionSetup>
    <remoteContainerConfiguration/>
    <simulationSetupCurrent>HNord</simulationSetupCurrent>
    <simulationSetups>
        <entry>
            <key>HNord</key>
            <value>hnord.xml</value>
        </entry>
    </simulationSetups>
    <subFolderEnvSetups>setupsEnv</subFolderEnvSetups>
</project>
